wait(.1)
ModMenuFrame1:TweenPosition(UDim2.new(0.2,0,0,0),"InOut","Sine",1)
ScriptsMenuSCALE:TweenPosition(UDim2.new(0,0,1,0),"InOut","Sine",1)
