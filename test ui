--load

wait(.1)
ModMenuFrame1:TweenPosition(UDim2.new(0.2,0,0,0),"InOut","Sine",1)
ScriptsMenuSCALE:TweenPosition(UDim2.new(0,0,1,0),"InOut","Sine",1)
GamesMenuSCALE:TweenPosition(UDim2.new(0,0,1,0),"InOut","Sine",1)

--close
Close1.MouseButton1Click:connect(function()
ScriptsMenuSCALE:TweenPosition(UDim2.new(0,0,2,0),"InOut","Sine",1)
end)
CLOSE2.MouseButton1Click:connect(function()
GamesMenuSCALE:TweenPosition(UDim2.new(0,0,2,0),"InOut","Sine",1)
end)

--hide
Hide1.MouseButton1Click:connect(function()
ScriptsMenuSCALE:TweenPosition(UDim2.new(0,0,1,0),"InOut","Sine",1)
end)
HIDE2.MouseButton1Click:connect(function()
GamesMenuSCALE:TweenPosition(UDim2.new(0,0,1,0),"InOut","Sine",1)
end)
  --hidebutton
HideButton.MouseButton1Click:connect(function()
ModMenuFrame1:TweenPosition(UDim2.new(1,0,0,0),"InOut","Sine",1)
GamesMenuSCALE:TweenPosition(UDim2.new(0,0,2,0),"InOut","Sine",1)
ScriptsMenuSCALE:TweenPosition(UDim2.new(0,0,2,0),"InOut","Sine",1)
wait(1)
CrybabyV1FE:Destroy()
end)


